package car;

import java.util.ArrayList;

public class Subsystem implements Car{
	String name;
	int totalPrice = 0;
	
	ArrayList subSystemsPriceArrayList=new ArrayList();
	
	public Subsystem(String subSystemName) {
		this.name=subSystemName;
	}
	
	public void addPart(String name,int price){
		subSystemsPriceArrayList.add(price);
		System.out.println(subSystemsPriceArrayList);
	}
	
	@Override
	public int getPrice() {
		// TODO Auto-generated method stub
		
		
		return totalPrice;
	}

}
