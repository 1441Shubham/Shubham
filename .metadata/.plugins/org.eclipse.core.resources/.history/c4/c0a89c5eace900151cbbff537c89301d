package proxyBank;

public class ProxyImage implements bank{

 String login = null;
	String bal = null;
 private RealImage obj;

	public void setlogin(String l){
		this.login = l;
	}
	
	public String getBalance() {
		if(login == null){
			System.out.println("Please Login!!");
		}else{
			bal = obj.getBalance();
		}
		return bal;
		
	}

	@Override
	public void withdraw(int amount) {
		if(login == null){
			System.out.println("Please Login!!");
		}else{
			 obj.withdraw(50);
		}
		
	}

	@Override
	public void deposit(int amount) {
		if(login == null){
			System.out.println("Please Login!!");
		}else{
			 obj.deposit(50);
		}
	}
	}