
public class stack {
	
	private int top=-1,maxSize=5;
	private int[] array = new int[maxSize];
	
	public String isFull(){
		String message="Array is full";
		if(top==maxSize-1){
			return message;
		}else{
			return null;
		}
	}
	
	public int push(int i){
		top++;
		array[top]=i;

		return array[top];	
	}
}
